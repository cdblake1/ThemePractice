<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nonprofitpmt"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/BaseTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".UserInformation"
            android:label="@string/title_activity_user_information"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.nonprofitpmt.MainActivity" />
        </activity>
        <activity
            android:name=".PaymentInformation"
            android:label="@string/title_activity_payment_information"
            android:parentActivityName=".MainActivity" >
        </activity>
        <activity
            android:name=".StartSearch"
            android:label="@string/title_activity_start_search" >
        </activity>
    </application>

</manifest>
